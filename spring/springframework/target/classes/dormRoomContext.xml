
<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">


    <context:annotation-config/>

    <!-- Define DormRoom beans -->
    <bean id="roomA" class="com.wipro.model.DormRoom">
        <constructor-arg value="10"/>
        <constructor-arg value="CD"/>
    </bean>

    <bean id="roomB" class="com.wipro.model.DormRoom">
        <constructor-arg value="12"/>
        <constructor-arg value="AB"/>
    </bean>

    <!-- Define Freshman bean -->
    <bean id="student" class="com.wipro.model.Freshman">
        <constructor-arg value="TEST"/>
    </bean>

</beans>
